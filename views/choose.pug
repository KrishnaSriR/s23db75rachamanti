extends layout

block content
  h1= title
  - var image_names = ["maths.jpg", "science.jpg", "history.jpg", "english.jpg", "biology.jpg"]
  - var chosen_images = []
  - for (var i = 0; i <3; i++)
    - var randomIndex = Math.floor(Math.random() * 5)
    - var image = image_names[randomIndex]
    img(src=`/images/${image}`, alt=`${image}`, height="300", width="300")

  